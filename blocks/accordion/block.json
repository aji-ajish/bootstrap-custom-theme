{
  "apiVersion": 2,
  "name": "bootstrap-custom-theme/accordion",
  "title": "Bootstrap Accordion",
  "category": "custom-blocks",
  "icon": "editor-accordion",
  "description": "A Bootstrap-style accordion with customizable settings.",
  "keywords": ["accordion", "bootstrap"],
  "attributes": {
    "accordionType": {
      "type": "string",
      "default": "accordion"
    },
    "numAccordions": {
      "type": "number",
      "default": 1
    },
    "accordionClass": {
      "type": "string",
      "default": ""
    },
    "accordionHeading0": {
      "type": "string",
      "default": "Accordion 1 Heading"
    }
  },
  "editorScript": "file:./build/index.js",
  "style": "file:./build/style-index.css"
}
