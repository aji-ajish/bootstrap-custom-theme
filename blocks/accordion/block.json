{
  "apiVersion": 2,
  "name": "bootstrap-custom-theme/accordion",
  "title": "Bootstrap Accordion",
  "category": "custom-blocks",
  "icon": "list-view",
  "description": "A Bootstrap-style accordion with customizable settings.",
  "keywords": ["accordion", "bootstrap", "collapse"],
  "attributes": {
    "accordionType": {
      "type": "string",
      "default": "accordion-flush"
    },
    "numAccordions": {
      "type": "number",
      "default": 1
    },
    "accordionClass": {
      "type": "string",
      "default": ""
    }
  },
  "supports": {
    "jsx": true
  },
  "editorScript": "file:./build/index.js",
  "style": "file:./build/style-index.css"
}
